  FormBox[GridBox[{
     {
      SubscriptBox["u", 
       RowBox[{"n", "-", "2"}]], "-", 
      RowBox[{"2", 
       SubscriptBox["u", 
        RowBox[{"n", "-", "1"}]]}], "+", 
      SubscriptBox["u", "n"], "=", 
