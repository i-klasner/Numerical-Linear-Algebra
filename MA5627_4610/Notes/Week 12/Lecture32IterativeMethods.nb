Cell[TextData[{
 "\nNow ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "0"], "=", "0"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "eb9acda2-fb80-4dc8-b899-70a8c6f68193"],
