Cell[BoxData[{
 RowBox[{
  RowBox[{"uExact", "[", "x_", "]"}], ":=", " ", 
  RowBox[{
   RowBox[{"Cos", "[", 
    RowBox[{
     RowBox[{"0.2", "x"}], "+", 
     RowBox[{"0.1", 
      RowBox[{"ArcTan", "[", "x", "]"}]}]}], "]"}], 
