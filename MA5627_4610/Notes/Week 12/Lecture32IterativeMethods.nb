Cell[CellGroupData[{

Cell["PDE Discretization 2D: Finite Differences (FD)", "Section",
 CellChangeTimes->{{3.857022210109374*^9, 3.857022250538159*^9}, {
  3.8570257918400583`*^9, 
  3.857025839637467*^9}},ExpressionUUID->"b5d28b5b-05bc-49e3-9c85-\
d16587da7d54"],

Cell[TextData[{
 "Consider the standard PDE\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["u", "xx"], "+", 
     SubscriptBox["u", "yy"]}], "=", "f"}], TraditionalForm]],ExpressionUUID->
  "48ea8f67-5eaf-4e05-9b54-6420a39f8ff8"],
 "\non the domain ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "\[Element]", "\[CapitalOmega]"}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"0", ",", "2"}], ")"}], "\[Times]", 
     RowBox[{"(", 
      RowBox[{"0", ",", "1"}], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "91979364-598f-4d06-ab6d-1d78687eb34d"],
 " with Dirichlet boundary conditions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"u", 
     SubscriptBox["|", 
      RowBox[{"\[PartialD]", "\[CapitalOmega]"}]]}], "=", "0"}], 
   TraditionalForm]],ExpressionUUID->"0c75b973-55d4-4c19-8715-37230790e52b"],
 " on the boundary ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[PartialD]", "\[CapitalOmega]"}], TraditionalForm]],
  ExpressionUUID->"6816b2d6-f288-4344-8055-fb919be4d5b7"],
 " of the domain ",
 Cell[BoxData[
  FormBox["\[CapitalOmega]", TraditionalForm]],ExpressionUUID->
  "d75ead05-8276-41f7-aa4e-09cca4304356"],
 ".   Standard centered difference approximations are\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["u", "xx"], "[", 
     RowBox[{"x", ",", "y"}], "]"}], "=", 
    FractionBox[
     RowBox[{
      RowBox[{"u", "[", 
       RowBox[{
        RowBox[{"x", "-", "h"}], ",", "y"}], "]"}], " ", "-", " ", 
      RowBox[{"2", " ", 
       RowBox[{"u", "[", 
        RowBox[{"x", ",", "y"}], "]"}]}], " ", "+", " ", 
      RowBox[{"u", "[", 
