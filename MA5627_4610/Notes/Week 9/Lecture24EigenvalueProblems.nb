Cell[BoxData[
 RowBox[{
  RowBox[{"GershgorignPic", "[", "A_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"m", "=", 
       RowBox[{"Length", "[", "A", "]"}]}], ",", "c", ",", "r"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Graphics", "[", 
     RowBox[{
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Table", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"c", "=", 
            RowBox[{"ReIm", "[", 
             RowBox[{"A", "\[LeftDoubleBracket]", 
              RowBox[{"i", ",", "i"}], "\[RightDoubleBracket]"}], "]"}]}], 
           ";", "\[IndentingNewLine]", 
           RowBox[{"r1", "=", 
            RowBox[{
             RowBox[{"Sum", "[", 
              RowBox[{
               RowBox[{"Abs", "[", 
                RowBox[{"A", "\[LeftDoubleBracket]", 
                 RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "]"}], 
               ",", 
               RowBox[{"{", 
                RowBox[{"j", ",", "1", ",", 
                 RowBox[{"i", "-", "1"}]}], "}"}]}], "]"}], "+", 
             RowBox[{"Sum", "[", 
              RowBox[{
               RowBox[{"Abs", "[", 
                RowBox[{"A", "\[LeftDoubleBracket]", 
                 RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "]"}], 
               ",", 
               RowBox[{"{", 
                RowBox[{"j", ",", 
                 RowBox[{"i", "+", "1"}], ",", "m"}], "}"}]}], "]"}]}]}], ";",
            "\[IndentingNewLine]", 
           RowBox[{"r2", "=", 
            RowBox[{
             RowBox[{"Sum", "[", 
              RowBox[{
               RowBox[{"Abs", "[", 
                RowBox[{"A", "\[LeftDoubleBracket]", 
                 RowBox[{"j", ",", "i"}], "\[RightDoubleBracket]"}], "]"}], 
               ",", 
               RowBox[{"{", 
                RowBox[{"j", ",", "1", ",", 
                 RowBox[{"i", "-", "1"}]}], "}"}]}], "]"}], "+", 
             RowBox[{"Sum", "[", 
              RowBox[{
               RowBox[{"Abs", "[", 
                RowBox[{"A", "\[LeftDoubleBracket]", 
                 RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "]"}], 
               ",", 
               RowBox[{"{", 
                RowBox[{"j", ",", 
                 RowBox[{"i", "+", "1"}], ",", "m"}], "}"}]}], "]"}]}]}], ";",
            "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"Hue", "[", 
              RowBox[{"i", "/", "m"}], "]"}], ",", 
             RowBox[{"Opacity", "[", "0.2", "]"}], ",", 
             RowBox[{"EdgeForm", "[", 
              RowBox[{"Hue", "[", 
               RowBox[{"i", "/", "m"}], "]"}], "]"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"Disk", "[", 
              RowBox[{"c", ",", 
               RowBox[{"Min", "[", 
                RowBox[{"r1", ",", "r2"}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"0", ",", 
                 RowBox[{"\[Pi]", "/", "2"}]}], "}"}]}], "]"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"Disk", "[", 
              RowBox[{"c", ",", 
               RowBox[{"Min", "[", 
                RowBox[{"r1", ",", "r2"}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"\[Pi]", "/", "2"}], ",", "\[Pi]"}], "}"}]}], "]"}], 
             ",", "\[IndentingNewLine]", 
             RowBox[{"Disk", "[", 
              RowBox[{"c", ",", 
               RowBox[{"Min", "[", 
                RowBox[{"r1", ",", "r2"}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"\[Pi]", ",", 
                 RowBox[{"3", 
                  RowBox[{"\[Pi]", "/", "2"}]}]}], "}"}]}], "]"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"Disk", "[", 
              RowBox[{"c", ",", 
               RowBox[{"Min", "[", 
                RowBox[{"r1", ",", "r2"}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"3", 
                  RowBox[{"\[Pi]", "/", "2"}]}], ",", 
                 RowBox[{"2", "\[Pi]"}]}], "}"}]}], "]"}]}], "}"}]}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"i", ",", "m"}], "}"}]}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Point", "[", 
         RowBox[{"ReIm", "[", 
          RowBox[{"Eigenvalues", "[", "A", "]"}], "]"}], "]"}]}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"Frame", "\[Rule]", "True"}], ",", "\[IndentingNewLine]", 
      RowBox[{"GridLines", "\[Rule]", "Automatic"}]}], "]"}]}], 
