Cell[TextData[{
 "\[OpenCurlyDoubleQuote]Every eigenvalue of A lies in at least one of the ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b40dfdff-ed6c-4b1e-9186-5d1911f6218e"],
 " circular disks with centers ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["c", "i"], "=", 
    RowBox[{"A", "\[LeftDoubleBracket]", 
     RowBox[{"i", ",", "i"}], "\[RightDoubleBracket]"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b4b8de26-27fd-41f5-8b09-546ceef11d81"],
 " and radius ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["r", "i"], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", 
      RowBox[{"j", "\[NotEqual]", "i"}]], 
     RowBox[{"|", 
      RowBox[{"A", "\[LeftDoubleBracket]", 
       RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "|"}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "1739ecfc-1113-4826-b693-da94bbb7175a"],
 ". If ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "32010164-50bb-44ea-9c94-3197d449f6e7"],
 " of the disks form a connected domain ",
 Cell[BoxData[
  FormBox["\[CapitalOmega]", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b1013733-4721-40bb-899a-a825aafbd135"],
 " disjoint from the other disks there are ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "552145bb-4b23-4e10-bca7-d010baf9762e"],
 " eigenvalues in ",
 Cell[BoxData[
  FormBox["\[CapitalOmega]", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8a8dfc85-44c7-4577-bb36-c941cbb5628a"],
 ".\[CloseCurlyDoubleQuote]"
}], "Text",
