                RowBox[{"r1", ",", "r2"}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"\[Pi]", ",", 
                 RowBox[{"3", 
                  RowBox[{"\[Pi]", "/", "2"}]}]}], "}"}]}], "]"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"Disk", "[", 
              RowBox[{"c", ",", 
               RowBox[{"Min", "[", 
                RowBox[{"r1", ",", "r2"}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"3", 
                  RowBox[{"\[Pi]", "/", "2"}]}], ",", 
                 RowBox[{"2", "\[Pi]"}]}], "}"}]}], "]"}]}], "}"}]}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"i", ",", "m"}], "}"}]}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Point", "[", 
