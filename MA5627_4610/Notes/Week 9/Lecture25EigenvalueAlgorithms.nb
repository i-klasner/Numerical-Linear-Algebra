Cell[TextData[{
 "We all know the quadratic formula for the roots of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"a", " ", 
      SuperscriptBox["\[Lambda]", "2"]}], "+", 
     RowBox[{"b", " ", "\[Lambda]"}], "+", "c"}], " ", "=", "0"}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7e8f4ab3-8157-4f75-b672-2ac7e89db144"],
 ". There is a formula for the three roots of a cubic.  Here is one version!  "
}], "Text",
 CellChangeTimes->{{3.8548863954580307`*^9, 3.854886468102641*^9}, {
  3.8548866047738123`*^9, 
  3.854886605682575*^9}},ExpressionUUID->"74e90d97-8ee3-48df-9ee5-\
69a96cf26afc"],

Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"a", ",", "b", ",", "c", ",", "\[Lambda]"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"\[Lambda]", "/.", 
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["\[Lambda]", "3"], "+", 
      RowBox[{"a", " ", 
       SuperscriptBox["\[Lambda]", "2"]}], "+", 
      RowBox[{"b", " ", "\[Lambda]"}], "+", "c"}], "==", "0"}], ",", 
    "\[Lambda]"}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.8548864699158993`*^9, 3.8548865279539623`*^9}},
 CellLabel->
  "In[389]:=",ExpressionUUID->"6e4a24a0-b80c-413c-a714-0b8501f53e56"],

