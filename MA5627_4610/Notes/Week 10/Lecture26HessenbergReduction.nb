Cell[TextData[{
 "Sweeping down the columns in a loop produces an almost upper triangular \
matrix with a single lower diagonal.  Accumulating these reflections into a \
single orthogonal matrix ",
 Cell[BoxData[
  FormBox["Q", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "817d9831-f78c-40c0-a541-24a6a7643051"],
 " gives a decomposition\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "=", 
    RowBox[{"Q", ".", "H", ".", 
     RowBox[{"Q", "\[Transpose]"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a81b190e-7d24-4662-b4fc-946e1f6b330d"],
 "\nwhere the inner matrix is upper triangular with one extra non-zero lower \
diagonal.  The inner matrix is called a Hessenberg matrix.  "
}], "Text",
 CellChangeTimes->{{3.854897967218526*^9, 3.8548979851045322`*^9}, {
   3.854900126943054*^9, 3.8549002921056747`*^9}, {3.854900326697633*^9, 
   3.854900410516254*^9}, 
   3.854900549909408*^9},ExpressionUUID->"aa2011c1-a75d-478a-90aa-\
b3711de7b03b"],

Cell["\<\
The eigenvalues of H are the same as the eigenvalues of A.  You do not build \
the orthogonal Q unless you need the eigenvectors.  in fact, you never need \
to build the Householder matrices.  This is a Mathematica implementation of \
algorithm 26.1.\
\>", "Text",
 CellChangeTimes->{{3.854900417555387*^9, 3.854900546253614*^9}, {
  3.854900892420333*^9, 3.854900927762014*^9}, {3.8549142365287743`*^9, 
  3.854914245987833*^9}},ExpressionUUID->"2541a53e-3839-4e9e-8240-\
0addcd416191"],

Cell[BoxData[
 RowBox[{
  RowBox[{"HessenbergReduction", "[", "AIn_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"A", "=", "AIn"}], ",", " ", 
      RowBox[{"m", "=", 
       RowBox[{"Length", "[", "A", "]"}]}], ",", "x"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"x", "=", 
         RowBox[{"A", "\[LeftDoubleBracket]", 
          RowBox[{
           RowBox[{
            RowBox[{"k", "+", "1"}], ";;", "m"}], ",", "k"}], 
          "\[RightDoubleBracket]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"v", "=", "x"}], ";", " ", 
        RowBox[{
         RowBox[{"v", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
         "=", 
         RowBox[{
          RowBox[{"v", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}],
           "+", 
          RowBox[{
           RowBox[{"Sign", "[", 
            RowBox[{
            "x", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
            "]"}], "*", 
           RowBox[{"Norm", "[", "x", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"v", "=", 
         RowBox[{"Normalize", "[", "v", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"A", "\[LeftDoubleBracket]", 
          RowBox[{
           RowBox[{
            RowBox[{"k", "+", "1"}], ";;", "m"}], ",", 
           RowBox[{"k", ";;", "m"}]}], "\[RightDoubleBracket]"}], "=", 
         RowBox[{
          RowBox[{"A", "\[LeftDoubleBracket]", 
           RowBox[{
            RowBox[{
             RowBox[{"k", "+", "1"}], ";;", "m"}], ",", 
            RowBox[{"k", ";;", "m"}]}], "\[RightDoubleBracket]"}], "-", 
          RowBox[{"2", " ", 
           RowBox[{"KroneckerProduct", "[", 
            RowBox[{"v", ",", 
             RowBox[{"v", ".", 
              RowBox[{"A", "\[LeftDoubleBracket]", 
               RowBox[{
                RowBox[{
                 RowBox[{"k", "+", "1"}], ";;", "m"}], ",", 
                RowBox[{"k", ";;", "m"}]}], "\[RightDoubleBracket]"}]}]}], 
            "]"}]}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"A", "\[LeftDoubleBracket]", 
          RowBox[{
           RowBox[{"1", ";;", "m"}], ",", 
           RowBox[{
            RowBox[{"k", "+", "1"}], ";;", "m"}]}], "\[RightDoubleBracket]"}],
          "=", 
         RowBox[{
          RowBox[{"A", "\[LeftDoubleBracket]", 
           RowBox[{
            RowBox[{"1", ";;", "m"}], ",", 
            RowBox[{
             RowBox[{"k", "+", "1"}], ";;", "m"}]}], 
           "\[RightDoubleBracket]"}], "-", 
          RowBox[{"2", " ", 
           RowBox[{"KroneckerProduct", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"A", "\[LeftDoubleBracket]", 
               RowBox[{
                RowBox[{"1", ";;", "m"}], ",", 
                RowBox[{
                 RowBox[{"k", "+", "1"}], ";;", "m"}]}], 
               "\[RightDoubleBracket]"}], ".", "v"}], ",", "v"}], 
            "]"}]}]}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"k", ",", "1", ",", 
         RowBox[{"m", "-", "2"}]}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Chop", "[", "A", "]"}]}]}], "]"}]}]], "Input",
 InitializationCell->True,
