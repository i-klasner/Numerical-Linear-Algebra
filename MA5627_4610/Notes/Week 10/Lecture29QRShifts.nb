     RowBox[{"MatrixPlot", "[", "H", "]"}]}], ",", "\[IndentingNewLine]", 
    RowBox[{"\"\<M\>\"", "->", 
     RowBox[{"MatrixPlot", "[", "M", "]"}]}], ",", "\[IndentingNewLine]", 
    RowBox[{"\"\<Q\[Transpose]MQ\>\"", "->", 
     RowBox[{"MatrixPlot", "[", "QtMQ", "]"}]}]}], "}"}], 
  "]"}], "\[IndentingNewLine]"}], "Input",
