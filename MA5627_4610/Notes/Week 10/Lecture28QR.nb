Cell[TextData[{
 "Householder reflections are good for introducing zeros in bulk. Remember \
Givens rotations\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
      {"p1", 
       RowBox[{"-", "p2"}]},
      {"p2", "p1"}
     }], ")"}], TraditionalForm]],ExpressionUUID->
  "3560384e-4c8f-464a-8223-1f59bb1ad093"],
 "\nand \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", "\[NoBreak]", GridBox[{
      {"p1", 
       RowBox[{"-", "p2"}], 
       RowBox[{"-", "p3"}], 
       RowBox[{"-", "p4"}]},
      {"p2", "p1", 
       RowBox[{"-", "p4"}], "p3"},
      {"p3", "p4", "p1", 
       RowBox[{"-", "p2"}]},
      {"p4", 
       RowBox[{"-", "p3"}], "p2", "p1"}
     }], "\[NoBreak]", ")"}], TraditionalForm]],ExpressionUUID->
  "6d25021f-fa46-44e7-a5e7-344e7dd7628a"],
 "\nand\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", "\[NoBreak]", GridBox[{
      {"p1", "p2", "p3", "p4", "p5", "p6", "p7", "p8"},
      {
       RowBox[{"-", "p2"}], "p1", "p4", 
       RowBox[{"-", "p3"}], "p6", 
       RowBox[{"-", "p5"}], 
       RowBox[{"-", "p8"}], "p7"},
      {
       RowBox[{"-", "p3"}], 
